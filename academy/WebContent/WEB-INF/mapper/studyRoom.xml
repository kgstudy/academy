<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="study">
	<insert id="newStudy" parameterType="java.util.HashMap">
		insert into studyroom values(study_seq.nextval, #{className}, #{grade}, #{classList}, #{teacher}, #{price}, #{day}, #{startTime}, #{endTime}, #{process}, #{book}, #{special}, #{subject}, #{startDate}, #{endDate})
	</insert>
	<select id="studyList" resultType="java.util.HashMap">
		select * from studyroom order by num asc
	</select>
	<select id="search" parameterType="java.util.HashMap" resultType="java.util.HashMap">
		select * from studyroom where startdate &lt;= #{date} and enddate &gt;= #{date} and subject = #{subject} order by num asc
	</select>
	<select id="searchAll" parameterType="java.util.HashMap" resultType="java.util.HashMap">
		select * from studyroom where startdate &lt;= #{date} and enddate &gt;= #{date} order by num asc
	</select>
</mapper>